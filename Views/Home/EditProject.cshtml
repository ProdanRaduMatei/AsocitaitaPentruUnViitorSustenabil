@model AsocitaitaPentruUnViitorSustenabil.Domain.Proiect

@{
    ViewData["Title"] = "Editeaza Proiect";
}

<h1>@ViewData["Title"]</h1>

<form asp-action="EditProject" asp-controller="Home" method="post">
    <div class="form-group">
        <label for="Id">ID Proiect</label>
        <input type="text" class="form-control" id="Id" name="Id" required />
    </div>
    <div class="form-group">
        <label for="Nume">Nume Proiect</label>
        <input type="text" class="form-control" id="Nume" name="Nume" required />
    </div>
    <button type="submit" class="btn btn-primary">Editeaza Proiect</button>
</form>